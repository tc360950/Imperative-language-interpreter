fun int (int, int) fun1(int z) {
	return ! int (int x, int y) {return x + y + z;};
}

fun int (int, int) zwrocona_anonimowa = fun1(10);
print(zwrocona_anonimowa(1, 2)); //powinno wypisac 13
print(zwrocona_anonimowa(2, 2)); //powinno wypisac 14

int fun2(int x, fun int (int) fun3) {
	return fun3(x) + 1;
}

int x = 10;

print(fun2(x, ! int (int x) {return x * 2;})); //wypisze 21


fun int(int &) x2 = ! int (int &x) {return x + 1;};

print(x2(x)) // wypisze 11
